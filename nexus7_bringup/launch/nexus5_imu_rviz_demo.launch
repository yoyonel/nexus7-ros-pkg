<?xml version="1.0"?>
<launch>
    <!-- Upload description -->
    <include file="$(find nexus7_description)/launch/upload_nexus5.launch" />
    <!-- Start imu to tf publisher -->
    <node pkg="nexus7_bringup" type="imu_to_tf.py" name="imu_to_tf" />
    <!-- Start rviz and load config -->
    <node pkg="rviz" type="rviz" output="screen" name="rviz" args="-d $(find nexus7_bringup)/config/imu_demo.rviz" />
</launch>
